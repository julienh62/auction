{% block create %}
{#  prepend apprait avant #}
    <turbo-stream action="prepend" target="table">
        <template>

            {%  include "auction/auction.html.twig" with {'auction' : entity} %}



        </template>
    </turbo-stream>

{% endblock %}

{% block update %}
    <turbo-stream action="replace" target="auction{{ id }}">
        <template>

            {%  include "auction/auction.html.twig" with {'auction' : entity} %}


        </template>
    </turbo-stream>
{% endblock %}

{% block remove %}
    <turbo-stream action="remove" target="auction{{ id }}"></turbo-stream>
{% endblock %}